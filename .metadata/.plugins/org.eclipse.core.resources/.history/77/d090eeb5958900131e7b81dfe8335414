package madcatBeans;

import java.util.ArrayList;

public class Segment {
	private String id;
	private ArrayList<String> zonerefs;
	private Token token;
	private String transcription;
	private String translation;
	
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public ArrayList<String> getZonerefs() {
		return zonerefs;
	}
	public void setZonerefs(ArrayList<String> zonerefs) {
		this.zonerefs = zonerefs;
	}
	
	
	
}
